{
  "name": "@aerogear/voyager-conflicts",
  "version": "0.7.1",
  "description": "A package to provide conflict handler framework for GraphQL server",
  "main": "dist/index.js",
  "scripts": {
    "compile": "tsc --build tsconfig.json",
    "watch": "tsc --build tsconfig.json --watch",
    "test": "echo 'no tests for conficts package'",
    "compile:clean": "tsc --build tsconfig.json --clean"
  },
  "author": "AeroGear Team<aerogear@googlegroups.com>",
  "license": "Apache-2.0",
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "files": [
      "**/*.test.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "ava": "2.4.0",
    "ts-node": "8.5.4",
    "typescript": "3.7.3"
  },
  "peerDependencies": {
    "graphql": "^0.13.0 || ^14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "offix-conflicts-server": "0.11.0"
  }
}
